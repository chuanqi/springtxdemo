<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xd.mapper.UserMapper">
   <insert id="insert" keyProperty="id" useGeneratedKeys="true" parameterType="com.xd.domain.User">
      INSERT INTO user(name,remark,update_time) VALUE(#{name},#{remark},#{updateTime})
   </insert>
</mapper>